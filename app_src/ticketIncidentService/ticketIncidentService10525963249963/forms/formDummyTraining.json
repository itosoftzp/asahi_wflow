{
    "className": "org.joget.apps.form.model.Form",
    "elements": [
        {
            "className": "org.joget.apps.form.model.Section",
            "elements": [{
                "className": "org.joget.apps.form.model.Column",
                "elements": [{
                    "className": "org.joget.apps.form.lib.CustomHTML",
                    "properties": {
                        "autoPopulate": "",
                        "id": "field2",
                        "label": "",
                        "value": "\n<style>\n    \n    .modal {\n        z-index: 875;\n    }\n    \n    .modal-backdrop {\n        z-index: 874;\n    }\n    .hideBtnSave {\n        display: none !important;\n    }\n    .form-check-btm {\n        padding-bottom: 20px;\n    }\n    .form-check {\n        padding-bottom: 20px;\n    }\n    .text-memo {\n        width: 200px;\n    }\n    .memo {\n        margin-top: -19px;\n    }\n    .icon-ownership {\n        width: 40px;\n    }\n    button#takeOwnerShip {\n        background-color: white;\n        padding: 0px;\n        border:0px;\n        /*border: 1px solid;*/\n        box-shadow: 3px 3px;\n        color: #E0E0E0;\n    }\n    button#actionss {\n        background-color: white;\n        padding: 0px;\n        border: 0px;\n        /*border: 1px solid;*/\n        box-shadow: 3px 3px;\n        color: #E0E0E0;\n    }\n    .tooltipBtn {\n      position: relative;\n      display: inline-block;\n    }\n    \n    .tooltipBtn .tooltiptext {\n      visibility: hidden;\n      width: 120px;\n      background-color: black;\n      color: #fff;\n      text-align: center;\n      border-radius: 6px;\n      padding: 5px 0;\n    \n      /* Position the tooltip */\n      position: absolute;\n      z-index: 1;\n      top: -29px;\n      left: -2px;\n    }\n    \n    .tooltipBtn:hover .tooltiptext {\n      visibility: visible;\n    }\n    \n<\/style>\n\n<div class=\"tooltipBtn\">\n  <button type=\"button\" id=\"actionss\" onclick=\"showModal()\"><img class=\"icon-ownership\" src=\"#appResource.icon_action2.png#\"><\/button>\n <span class=\"tooltiptext\">Action<\/span>\n<\/div>\n<div class=\"tooltipBtn\">\n <button type=\"button\" id=\"takeOwnerShip\"><img class=\"icon-ownership\" src=\"#appResource.icon_take_ownership3.png#\"><\/button>\n <span class=\"tooltiptext\">Take Owner<\/span>\n<\/div>\n\n<!-- Modal Analysis -->\n<div class=\"modal fade\" id=\"modalAnalysis\"  tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLongTitle\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">Analysis Action<\/h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;<\/span>\n        <\/button>\n      <\/div>\n      <div class=\"modal-body\">\n        <div class=\"form-check\">\n          <label class=\"form-check-label\" for=\"analystRedis\">\n            REDISPATCH BY SELECT SOLUTION AND ROUTE TO BACKEND\n          <\/label>\n          <input class=\"form-check-input\" type=\"radio\" name=\"analysisOption\" id=\"analysisRedispathToBackend\" value=\"analysisRedispathToBackend\">\n        <\/div>\n        \n        <div class=\"form-check\">\n          <label class=\"form-check-label\">\n            REDISPATCH BY REASSIGN OWNER / OWNERGROUP\n          <\/label>\n          <input class=\"form-check-input\" type=\"radio\" name=\"analysisOption\" id=\"analysisToOwnerGroup\" value=\"analysisToOwnerGroup\">\n        <\/div>\n        \n        <div class=\"form-check\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"analysisOption\" id=\"analysisToRequestPending\" value=\"analysisToRequestPending\">\n          <label class=\"form-check-label\" for=\"analystToPending\">\n           REQUEST PENDING\n          <\/label>\n        <\/div>\n        \n        <div class=\"form-check\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"analysisOption\" id=\"analysisToFinalcheck\" value=\"analysisToFinalcheck\">\n          <label class=\"form-check-label\" for=\"analystToFinalCheck\">\n           RESOLVE TIKET\n          <\/label>\n        <\/div>\n        \n        <div class=\"memo\">\n            <label class=\"text-memo\">Memo<\/label>\n            <input class=\"text-memo\" type=\"text\" name=\"memoAnalyst\" id=\"memoAnalyst\" />\n        <\/div>\n      <\/div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close<\/button>\n        <button type=\"button\" class=\"btn btn-primary\" onclick=\"updateStatusAnalyst()\">Save<\/button>\n      <\/div>\n    <\/div>\n  <\/div>\n<\/div>\n\n\n<!-- Modal Backend -->\n<div class=\"modal fade\" id=\"modalBackend\"  tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLongTitle\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">Backend Action<\/h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;<\/span>\n        <\/button>\n      <\/div>\n      <div class=\"modal-body\">\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"backendOption\" id=\"backendToRelevanSolution\" value=\"backendToRelevanSolution\">\n          <label class=\"form-check-label\" for=\"backendToRelevanSolution\">\n              REDISPATCH BY SELECT SOLUTION\n          <\/label>\n        <\/div>\n        \n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"backendOption\" id=\"backendToReassignOwner\" value=\"backendToReassignOwner\">\n          <label class=\"form-check-label\" for=\"backendToReassignOwner\">\n               REDISPATCH BY REASSIGN OWNER/OWNERGROUP\n          <\/label>\n        <\/div>\n        \n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"backendOption\" id=\"backendToFinalCheck\" value=\"backendToFinalCheck\">\n          <label class=\"form-check-label\" for=\"backendToFinalCheck\">RESOLVE TIKET<\/label>\n        <\/div>\n        \n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"backendOption\" id=\"backendToPending\" value=\"backendToPending\">\n          <label class=\"form-check-label\" for=\"backendToPending\">REQUEST PENDING<\/label>\n        <\/div>\n        \n        \n        <div>\n            <label>Memo<\/label>\n            <input type=\"text\" name=\"memoBackend\" id=\"memoBackend\" />\n        <\/div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close<\/button>\n        <button type=\"button\" class=\"btn btn-primary\" onclick=\"updateStatusBackend()\">Save<\/button>\n      <\/div>\n    <\/div>\n  <\/div>\n  <\/div>\n<\/div>\n\n\n<!-- Modal Final Check -->\n<div class=\"modal fade\" id=\"modalFinalCheck\"  tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLongTitle\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">FinalCheck Action<\/h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;<\/span>\n        <\/button>\n      <\/div>\n      <div class=\"modal-body\">\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"finalcheckOption\" id=\"finalcheckToBySelectSolution\" value=\"finalcheckToBySelectSolution\">\n          <label class=\"form-check-label\" for=\"finalcheckToBySelectSolution\">\n            REDISPATCH BY SELECT SOLUTION\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"finalcheckOption\" id=\"finalcheckToReassignOwner\" value=\"finalcheckToReassignOwner\">\n          <label class=\"form-check-label\" for=\"finalcheckToReassignOwner\">\n            REDISPATCH BY REASSIGN OWNER / OWNERGROUP\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"finalcheckOption\" id=\"finalcheckToBeforeProcess\" value=\"finalcheckToBeforeProcess\">\n          <label class=\"form-check-label\" for=\"finalcheckToBeforeProcess\">\n           REOPEN/REWORK TIKET\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"finalcheckOption\" id=\"finalcheckToMediacare\" value=\"finalcheckToMediacare\">\n          <label class=\"form-check-label\" for=\"finalcheckToMediacare\">\n           RESOLVE TIKET\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"finalcheckOption\" id=\"finalcheckToPending\" value=\"finalcheckToPending\">\n          <label class=\"form-check-label\" for=\"finalcheckToPending\">\n           REQUEST PENDING\n          <\/label>\n        <\/div>\n        <div>\n            <label>Memo<\/label>\n            <input type=\"text\" name=\"memoFinal\" id=\"memoFinal\" />\n        <\/div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close<\/button>\n        <button type=\"button\" class=\"btn btn-primary\" onclick=\"updateStatusFinalCheck()\">Save<\/button>\n      <\/div>\n    <\/div>\n  <\/div>\n  <\/div>\n<\/div>\n\n\n<!-- Modal Salam Sim-->\n<div class=\"modal fade\" id=\"modalSalamSim\"  tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLongTitle\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">Salam Simpatik Action<\/h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;<\/span>\n        <\/button>\n      <\/div>\n      <div class=\"modal-body\">\n       <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"salamOption\" id=\"salamsimToBeforeProcess\" value =\"salamsimToBeforeProcess\">\n          <label class=\"form-check-label\" for=\"salamsimToBeforeProcess\">\n            REOPEN/REWORK TIKET\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"salamOption\" id=\"salamsimToClosed\" value=\"salamsimToClosed\">\n          <label class=\"form-check-label\" for=\"salamsimToClosed\">\n           RESOLVE TIKET\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"salamOption\" id=\"salamsimToPending\" value=\"salamsimToPending\">\n          <label class=\"form-check-label\" for=\"salamsimToPending\">\n           REQUEST PENDING\n          <\/label>\n        <\/div>\n        <div>\n            <label>Memo<\/label>\n            <input type=\"text\" name=\"memoSalam\" id=\"memoSalam\" />\n        <\/div>\n      <\/div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close<\/button>\n        <button type=\"button\" class=\"btn btn-primary\" onclick=\"updateStatusSalamSim()\">Save<\/button>\n      <\/div>\n    <\/div>\n  <\/div>\n<\/div>\n\n\n<!-- Modal Media Caring-->\n<div class=\"modal fade\" id=\"modalMediacare\"  tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLongTitle\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">Media Caring Action<\/h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;<\/span>\n        <\/button>\n      <\/div>\n      <div class=\"modal-body\">\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"mediacareOption\" id=\"mediacareToBeforeProcess\" value =\"mediacareToBeforeProcess\">\n          <label class=\"form-check-label\" for=\"mediaCareToSalamSim\">\n            REOPEN/REWORK TIKET\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"mediacareOption\" id=\"mediaCareToClose\" value=\"mediaCareToClose\">\n          <label class=\"form-check-label\" for=\"mediaCareToClose\">\n           RESOLVE TIKET\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"mediacareOption\" id=\"mediaCareToPending\" value=\"mediaCareToPending\">\n          <label class=\"form-check-label\" for=\"mediaCareToPending\">\n           REQUEST PENDING\n          <\/label>\n        <\/div>\n        <div>\n            <label>Memo<\/label>\n            <input type=\"text\" name=\"memoMedia\" id=\"memoMedia\" />\n        <\/div>\n      <\/div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close<\/button>\n        <button type=\"button\" class=\"btn btn-primary\" onclick=\"updateStatusMediaCaring()\">Save<\/button>\n      <\/div>\n    <\/div>\n  <\/div>\n<\/div>\n\n<!-- Modal Media Caring-->\n<div class=\"modal fade\" id=\"modalRequestPending\"  tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLongTitle\" aria-hidden=\"true\">\n  <div class=\"modal-dialog\" role=\"document\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">Request Pending Action<\/h5>\n        <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span aria-hidden=\"true\">&times;<\/span>\n        <\/button>\n      <\/div>\n      <div class=\"modal-body\">\n        <div class=\"form-check-btm\">\n         <input class=\"form-check-input\" type=\"radio\" name=\"requestPendingOption\" id=\"requestPendingApproved\" value =\"requestPendingApproved\">\n          <label class=\"form-check-label\">\n            APPROVED\n          <\/label>\n        <\/div>\n        <div class=\"form-check-btm\">\n          <input class=\"form-check-input\" type=\"radio\" name=\"requestPendingOption\" id=\"requestPendingReject\" value=\"requestPendingReject\">\n          <label class=\"form-check-label\">\n            REJECTED\n          <\/label>\n        <\/div>\n        <div>\n            <label>Memo<\/label>\n            <input type=\"text\" name=\"memoRequestPending\" id=\"memoRequestPending\" />\n        <\/div>\n      <\/div>\n      <div class=\"modal-footer\">\n        <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close<\/button>\n        <button type=\"button\" class=\"btn btn-primary\" onclick=\"updateStatusRequestPending()\">Save<\/button>\n      <\/div>\n    <\/div>\n  <\/div>\n<\/div>\n\n<script>\n    let currentProcess = '';\n    let actionStatus ='';\n    let ticketStatus = '';\n    $(document).ready(function(){\n        actionStatus = $(\"#child_id_1_viewTicketSubFormAfterRunning_action_status\").val();\n        // $(\"input[name='submit']\").hide();\n        $(\"input[name='submit']\").addClass('hideBtnSave');\n        \n        $(\"#takeOwnerShip\").click(() => {\n            $(\"#child_id_1_viewTicketSubFormAfterRunning_owner\").val('#currentUser.username#');\n            $(\"input[name='submit']\").click();\n        })\n    // const parent = document.querySelector('.child_id_1_viewTicketSubFormAfterRunning_actual_solution').parentNode.parentNode.childNodes[4].childNodes[0];\n    // document.querySelector('.child_id_1_viewTicketSubFormAfterRunning_actual_solution').parentNode.parentNode.childNodes[4].childNodes[0].click()\n\n    // alert(parent)\n    });\n    \n    \n\tconst showModal = () => {\n\t   if(currentProcess === 'analysis' && actionStatus !== 'REQUEST_PENDING') {\n            $('#modalAnalysis').modal('show');\n        }\n        if(currentProcess === 'pending' && actionStatus !== 'REQUEST_PENDING') {\n            $('#modalPending').modal('show');\n        }\n        if(currentProcess === 'backend' && actionStatus !== 'REQUEST_PENDING') {\n            $('#modalBackend').modal('show');\n        }\n        if(currentProcess === 'finalcheck' && actionStatus !== 'REQUEST_PENDING') {\n            $('#modalFinalCheck').modal('show');\n        }\n        if(currentProcess === 'mediacare' && actionStatus !== 'REQUEST_PENDING') {\n            $('#modalMediacare').modal('show');\n        }\n        if(currentProcess === 'salamsim' && actionStatus !== 'REQUEST_PENDING') {\n            $('#modalSalamSim').modal('show');\n        }\n        if(actionStatus === 'REQUEST_PENDING') {\n        \t$('#modalRequestPending').modal('show');\n        }\n\t    \n\t}\n    \n    \n        let reqData = {\n\t\t\tticket_id:'',\n\t\t\tclass:'',\n\t\t\tstatus:'',\n\t\t\tchange_by:'',\n\t\t\tchange_date:'',\n\t\t\tmemo:'',\n\t\t\tsite_idorg_id:'',\n\t\t\ttk_status_id:'',\n\t\t\tstatus_tracking:'',\n\t\t\towner:'',\n\t\t\towner_group:'',\n\t\t\tassigned_owner_group:''\n\t\t}\n\t\t\n\t\n\t\t//ANALYSIS OPTION\n        $(\"input[name='analysisOption']\").change(() => {\n            const status = $(\"input[name='analysisOption']:checked\").val();\n            $('#child_id_1_viewTicketSubFormAfterRunning_last_state').val('ANALYSIS');\n            if(status === 'analysisRedispathToBackend') {\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_ticket_status').val(\"BACKEND\");\n            \t$(\"input[name='child_id_1_viewTicketSubFormAfterRunning_status']\").val(\"BACKEND\");\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_action_status').val(\"REDISPATCH_TO_BACKEND\")\n            \tticketStatus = 'BACKEND';\n            \t//masih harus ditambahkan option solutionnya\n            }\n            if(status === 'analysisToOwnerGroup') {\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_ticket_status').val(\"ANALYSIS\");\n            \t$(\"input[name='child_id_1_viewTicketSubFormAfterRunning_status']\").val(\"ANALYSIS\");\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_action_status').val(\"REASSIGN_OWNER\");\n                ticketStatus = 'ANALYSIS';\n                //masih harus ditambahkan option solutionnya\n            }\n            if(status === 'analysisToRequestPending') {\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_ticket_status').val(\"ANALYSIS\");\n            \t$(\"input[name='child_id_1_viewTicketSubFormAfterRunning_status']\").val(\"ANALYSIS\");\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_action_status').val(\"REQUEST_PENDING\")\n                ticketStatus = 'ANALYSIS';\n            }\n            if(status === 'analysisToFinalcheck') {\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_ticket_status').val(\"FINALCHECK\");\n            \t$(\"input[name='child_id_1_viewTicketSubFormAfterRunning_status']\").val(\"FINALCHECK\");\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_action_status').val(\"RESOLVE\")\n            \tticketStatus = 'FINALCHECK';\n            }\n        });\n        \n        // \t\t//BACKEND OPTION\n        $(\"input[name='backendOption']\").change(() => {\n            const status = $(\"input[name='backendOption']:checked\").val();\n            $('#child_id_1_viewTicketSubFormAfterRunning_last_state').val('BACKEND');\n            if(status === 'backendToRelevanSolution') {\n            \t$('#child_id_1_viewTicketSubFormAfterRunning_ticket_status').val(\"BACKEND\");\n            \t$(\"input[name='child_id_1_viewTicketSubFormAfterRunning_status']\").val(\"BACKEND\");\n            \t$('